<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Register">
    <children>
        <TextField fx:id="username" layoutX="215.0" layoutY="104.0" promptText="username" />
        <TextField fx:id="full_name" layoutX="215.0" layoutY="155.0" promptText="full name" />
        <PasswordField fx:id="password" layoutX="215.0" layoutY="200.0" promptText="password" />
        <Button layoutX="215.0" layoutY="240.0" mnemonicParsing="false" onAction="#btnSubmit" prefHeight="26.0" prefWidth="71.0" text="Submit" />
        <Button layoutX="315.0" layoutY="240.0" mnemonicParsing="false" onAction="#btnBack" prefHeight="26.0" prefWidth="71.0" text="Back" />
    </children>
</AnchorPane>
